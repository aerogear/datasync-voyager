{
  "name": "@aerogear/voyager-client",
  "version": "3.0.0",
  "description": "AeroGear Voyager GraphQL client",
  "author": "AeroGear Team<aerogear@googlegroups.com>",
  "homepage": "https://github.com/aerogear/voyager-server#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aerogear/voyager-server/tree/master/packages/voyager-client"
  },
  "keywords": [
    "aerogear",
    "aerogear services",
    "graphql"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "tsc --build tsconfig.json",
    "watch": "tsc --build tsconfig.json --watch",
    "compile:clean": "tsc --build tsconfig.json --clean",
    "test": "ava -v"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "offix-client-boost": "0.15.1"
  },
  "devDependencies": {
    "@types/jest": "25.2.3",
    "ts-jest": "26.1.0",
    "jest": "26.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.0.2"
  },
  "peerDependencies": {
    "graphql": "^0.13.0 || ^14.0.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.test.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
